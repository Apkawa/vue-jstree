{
  "name": "vue-jstree",
  "version": "2.2",
  "description": "a tree plugin for vue2",
  "keywords": [
    "vue2",
    "tree"
  ],
  "author": "zdy1988 <virus_zhh@126.com>",
  "main": "dist/vue-jstree.js",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zdy1988/vue-jstree.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.examples.config.js --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "gh-pages:build": "cross-env NODE_ENV=production webpack --config webpack.examples.config.js --progress --hide-modules",
    "gh-pages:publish": "gh-pages-deploy",
    "clean-source": "rimraf README.md src webroot package.json",
    "prepublish": "npm publish",
    "lint": "eslint src"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "style-loader": "^0.18.2",
    "vue": "^2.5.16",
    "vue-loader": "^12.1.0",
    "vue-router": "^3.0.1",
    "vue-router-nav": "^0.1.8",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "gh-pages-deploy": {
    "staticpath": "demo",
    "commit": "a custom commit message",
    "prep": [
      "gh-pages:build"
    ],
    "post": [
      "clean-source"
    ],
    "noprompt": false
  }
}
